@model SUBDCORE.Models.Position

@{
    ViewData["Title"] = "Create";
}

<h3>Добавление Должности</h3>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label>Наименование должности</label>
                <input asp-for="Position1" class="form-control" />
                <span asp-validation-for="Position1" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Добавить" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Вернуться назад</a>
</div>

