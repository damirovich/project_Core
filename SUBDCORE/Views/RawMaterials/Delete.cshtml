@model SUBDCORE.Models.RawMaterials

@{
    ViewData["Title"] = "Delete";
}

<h4>Удаления Сырье</h4>

<h5>Вы действительно хотите удалить это Сырье?</h5>
<div>
    <table class="table">
        <tr>
            <td>
                <label>Наименование Сырье</label>
                <h4>
                    @Html.DisplayFor(model => model.Names)
                </h4>
            </td>
            <td>
                <label>Единица измерения</label>
                <h4>
                    @Html.DisplayFor(model => model.UnitOfMeasureNavigation.Names)
                </h4>
            </td>
            <td>
                <label>Сумма</label>
                <h4>
                    @Html.DisplayFor(model => model.Summ)
                </h4>
            </td>
            <td>
                <label>Колличество</label>
                <h4>
                    @Html.DisplayFor(model => model.Quantity)
                </h4>
            </td>
        </tr>
    </table>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdRawMaterials" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
    </form>
    <div>
        <a asp-action="Index">Вернуться назад</a>
    </div>
</div>
