@model SUBDCORE.Models.Credits

@{
    ViewData["Title"] = "Delete";
}

<h4>Вы действительно хотите удалить этот запись ?</h4>
<div>
    <table class="table">
        <tr>
            <td>
                <label>Наименование Банка</label>
                <h5>
                    @Html.DisplayFor(model => model.Name)
                </h5>
            </td>
            <td>
                <label>Сумма кредита</label>
                <h5>
                    @Html.DisplayFor(model => model.CreaditAmount)
                </h5>
            </td>
            <td>
                <label>Процентная ставка</label>
                <h5>
                    @Html.DisplayFor(model => model.InterestRate)
                </h5>
            </td>
            <td>
                <label>Срок кредита</label>
                <h5>
                    @Html.DisplayFor(model => model.CreditTerm)
                </h5>
            </td>
            <td>
                <label>Дата вязите кредита</label>
                <h5>
                    @Html.DisplayFor(model => model.DateOfIssue)
                </h5>
            </td>
        </tr>
    </table>
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdCredits" />
        <input type="submit" value="Delete" class="btn btn-danger" /> 
    </form>
    <div>
        <a asp-action="Index">Вернуться назад</a>
    </div>
</div>
